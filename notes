git config --global user.name "your name"
git config --global user.email "your email"

git config --global push.default matching
git config --global alias.co checkout

git init

#for changes and updates in code.
git add .
git commit -am  "message for github"

# if issue with origin, use = git remote rm origin
git remote add origin "repo code without inverted commas"
git branch -M main
git push origin main